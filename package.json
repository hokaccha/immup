{
  "name": "immup",
  "version": "1.0.0",
  "description": "Update deeply nested objects/arrays without changing the original source",
  "main": "lib/immup.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hokaccha/immup.git"
  },
  "files": [
    "README.md",
    "lib",
    "src"
  ],
  "scripts": {
    "build": "babel src --out-dir lib --source-maps",
    "prepublish": "npm run build",
    "test": "ava -v test",
    "lint": "eslint --max-warnings 0 --ignore-path .eslintignore .",
    "ci": "npm test && npm run lint"
  },
  "bugs": {
    "url": "https://github.com/hokaccha/immup/issues"
  },
  "homepage": "https://github.com/hokaccha/immup",
  "author": "Kazuhito Hokamura",
  "license": "MIT",
  "devDependencies": {
    "@hokaccha/eslint-config": "^2.1.0",
    "ava": "^0.17.0",
    "babel-cli": "^6.18.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.18.0",
    "eslint": "^3.12.2"
  },
  "ava": {
    "require": [
      "babel-register"
    ]
  }
}
